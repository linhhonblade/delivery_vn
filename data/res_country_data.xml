<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <record id="base.vn" model="res.country">
        <field name="name">Vietnam</field>
        <field name="code">vn</field>
        <field name="currency_id" ref="base.VND"/>
        <field eval="84" name="phone_code" />
        <field name="zip_required">0</field>
        <field
                eval="'%(street)s\n%(street2)s\n%(ward_name)s %(district_name)s\n%(state_name)s %(country_name)s'" name="address_format"/>
        <field eval="ref('delivery_vn.view_partner_address_format_vn')" name="address_view_id"/>
    </record>
    </data>
</odoo>
